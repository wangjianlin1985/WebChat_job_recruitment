<view class="container">
  <loading hidden="{{loadingHide}}">
    {{loadingText}}
  </loading>

  <view class="title">商家评价详情</view>
  <view class="evaluate_detail_view"> 
      <view class="viewItem">
        <view class="label">评价id：</view>
        <view class="content">
          <view>{{evaluateId}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">被评价商家：</view>
        <view class="content"> 
          <view>{{sellerObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评分：</view>
        <view class="content"> 
          <view>{{evaluateScore}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评价内容：</view>
        <view class="content"> 
          <view>{{evaluateContent}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评论用户：</view>
        <view class="content"> 
          <view>{{userObj}}</view>
        </view>
      </view>

       <view class="viewItem">
        <view class="label">评价时间：</view>
        <view class="content"> 
          <view>{{evaluateTime}}</view>
        </view>
      </view>

      <view>
        <navigator class="btnReturn" open-type="navigateBack" hover-class="navigator-hover">返回</navigator>
      </view>

  </view>
</view>
